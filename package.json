  },
  "keywords": [
    "textlint"
  ],
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-jsdoc-to-assert": "^2.0.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "mocha": "^2.5.3",
    "power-assert": "^1.4.1",
    "textlint-tester": "^1.2.0"
  },
  "dependencies": {
    "kuromojin": "^1.3.1",
    "morpheme-match": "^1.0.1"
  }
}